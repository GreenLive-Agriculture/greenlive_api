package osc.greenlive.model;

import java.util.ArrayList;
import java.util.List;

import jakarta.persistence.FetchType;
import jakarta.persistence.Id;
import jakarta.persistence.ManyToMany;

public class Kit {
	
	private String name_kit;
	
	@Id
	private Long id_kit;
	
	private String type_kit ;
	
	@ManyToMany(fetch=FetchType.LAZY)
	private List<Cultures> kit_culture = new ArrayList<>();
	
	@ManyToMany(mappedBy="kit_culture",fetch=FetchType.EAGER)
	private List<Kit> kit_ressource = new ArrayList<>();
}
